#!/bin/sh

# Dependancies
sudo apt-get install libsdl2-2.0 libsdl2-dev libalut-dev

# Installing Ryujinx file
google-chrome https://github.com/Ryujinx/release-channel-master/releases/download/1.1.15/ryujinx-1.1.15-linux_x64.tar.gz

# Installing product keys
google-chrome http://download2343.mediafire.com/5dsiikriyxbg/pnqxkz7ae47pvjm/prod.keys

# Installing firmware 
google-chrome https://mega.nz/file/xYYkyT7K#I0Xr60_co04X_JWUirfVyswg0pR_XnlxeIDMK5YHEYQ

# Extracting and runnning switch file
cd Downloads
gunzip ryujinx-1.1.15-linux_x64.tar.gz
tar xvf ryujinx-1.1.15-linux_x64.tar
cd publish
chmod 777 Ryujinx

